{
  "order": 60,
  "index_patterns": [
    "ecs-bro-network-rfb-*"
  ],
  "mappings": {
    "properties": {
      "rfb": {
        "properties": {
          "authentication": {
            "properties": {
              "method": {
                "ignore_above": 1024,
                "fields": {
                  "text": {
                    "type": "text"
                  }
                },
                "type": "keyword"
              },
              "successful": {
                "type": "boolean"
              }
            }
          },
          "desktop_height": {
            "type": "long"
          },
          "desktop_width": {
            "type": "long"
          },
          "share_flag": {
            "type": "boolean"
          }
        }
      }
    }
  }
}
